{
  "routes": [
    {
      "route": "/api/*",
      "allowedRoles": ["authenticated"],
      // __PLACEHOLDER_BACKEND_API_URL__ â€“ backend Functions App URL if using proxying
      "rewrite": "/api/__PLACEHOLDER_BACKEND_REWRITE__"
